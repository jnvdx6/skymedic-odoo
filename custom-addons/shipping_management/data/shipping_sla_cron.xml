<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record forcecreate="True" id="ir_cron_shipping_sla_check"
            model="ir.cron">
        <field name="name">Envíos: alerta SLA / envíos estancados</field>
        <field name="model_id" ref="model_shipping_shipment"/>
        <field name="state">code</field>
        <field name="code">model._cron_check_sla_alerts()</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
    </record>

</odoo>
