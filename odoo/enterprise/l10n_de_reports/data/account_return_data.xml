<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="de_tax_return_type" model="account.return.type">
            <field name="name">Tax</field>
            <field name="report_id" ref="l10n_de.tax_report"/>
            <field name="country_id" ref="base.de" />
            <field name="default_deadline_days_delay">10</field>
        </record>

        <!-- EC sales list -->
        <record id="de_ec_sales_list_return_type" model="account.return.type">
            <field name="name">EC Sales List</field>
            <field name="report_id" ref="l10n_de_reports.german_ec_sales_report"/>
            <field name="country_id" ref="base.de"/>
            <field name="auto_generate" eval="False"/>
        </record>

    </data>
</odoo>
