<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="pl_tax_return_type" model="account.return.type">
            <field name="name">Tax</field>
            <field name="report_id" ref="l10n_pl.tax_report"/>
            <field name="country_id" ref="base.pl" />
            <field name="default_deadline_days_delay">25</field>
        </record>

        <record id="pl_ec_sales_list_return_type" model="account.return.type">
            <field name="name">EC Sales List</field>
            <field name="report_id" ref="account_reports.generic_ec_sales_report"/>
            <field name="country_id" ref="base.pl"/>
            <field name="auto_generate" eval="False"/>
        </record>

    </data>
</odoo>
