<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="at_tax_return_type" model="account.return.type">
            <field name="name">Tax</field>
            <field name="report_id" ref="l10n_at.tax_report"/>
            <field name="country_id" ref="base.at" />
            <field name="default_deadline_days_delay">15</field>
        </record>

        <!-- EC sales list -->
        <record id="at_ec_sales_list_return_type" model="account.return.type">
            <field name="name">EC Sales List</field>
            <field name="report_id" ref="l10n_at_reports.austrian_ec_sales_report"/>
            <field name="country_id" ref="base.at"/>
            <field name="auto_generate" eval="False"/>
        </record>

    </data>
</odoo>
