<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="default_sale_product" model="product.product">
        <field name="name">Lazada Sale</field>
        <field name="list_price">0.0</field>
        <field name="sale_ok">False</field>
        <field name="purchase_ok">False</field>
    </record>

    <record id="default_shipping_product" model="product.product">
        <field name="name">Lazada Shipping</field>
        <field name="type">service</field>
        <field name="list_price">0.0</field>
        <field name="sale_ok">False</field>
        <field name="purchase_ok">False</field>
    </record>

    <function model="product.product" name="_configure_for_lazada">
        <value eval="[
            ref('sale_lazada.default_sale_product'), ref('sale_lazada.default_shipping_product')
        ]"/>
    </function>

</odoo>
