<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="fi_tax_return_type" model="account.return.type">
            <field name="name">Tax</field>
            <field name="report_id" ref="l10n_fi.vat_report"/>
            <field name="country_id" ref="base.fi" />
            <field name="states_workflow">generic_state_review_submit</field>
        </record>

        <record id="fi_ec_sales_list_return_type" model="account.return.type">
            <field name="name">EC Sales List</field>
            <field name="report_id" ref="l10n_fi_reports.finland_ec_sales_report"/>
            <field name="default_deadline_periodicity">monthly</field>
            <field name="auto_generate" eval="False"/>
            <field name="default_deadline_days_delay">20</field>
        </record>
    </data>
</odoo>
